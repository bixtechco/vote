{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^8.0",
        "api-ecosystem-for-laravel/dingo-api": "*",
        "askedio/laravel5-soft-cascade": "10.0",
        "barryvdh/laravel-ide-helper": "^2.4",
        "cakephp/utility": "^3.6",
        "diglactic/laravel-breadcrumbs": "^8.1",
        "doctrine/dbal": "^3.0",
        "guzzlehttp/guzzle": "^7.3",
        "inspector-apm/inspector-laravel": "^4.4",
        "intervention/image": "^2.4",
        "laracasts/flash": "^3.0",
        "laravel/framework": "^10",
        "laravel/helpers": "^1.1",
        "laravel/legacy-factories": "^1.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "*",
        "maatwebsite/excel": "*",
        "silber/bouncer": "v1.0.*",
        "stripe/stripe-php": "*",
        "tymon/jwt-auth": "2.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.6",
        "filp/whoops": "^2.0",
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/",
            "Diver\\": "diver/",
            "Src\\": "src/"
        },
        "files": [
            "diver/Helpers/Data.php",
            "diver/Helpers/Hash.php",
            "diver/Helpers/Text.php",
            "app/Helpers/Helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
              "laravel/dusk"
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta",
            "@php artisan ide-helper:model --nowrite"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
